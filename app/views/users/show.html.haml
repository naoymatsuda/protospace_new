.container.user-page
  %header.user-nav
    .media
      .media-left
        = image_tag @user.avatar ,class: 'media-object' , alt: "64x64", data:{ src: "holder.js/64x64", holder:{rendered: true}}, size: "64x64"
      .media-body
        %h4#top-aligned-media.media-heading
          = @user.name
          %span.label.label-default.project-number
            29
            %i Projects
        .proto-user
          %span.degree
          = @user.member
        .row
          .col-md-6
            %h4 Profile
            %p= @user.profile
          .col-md-6
            %h4 Works
            %p= @user.works
.container.proto-list
  .row
    = render partial: 'prototypes/prototype', collection: @prototypes
.text-center
  %ul.pagination
    %li.disabled
      %a{"aria-label": "Previous", href: "#"}
        %span{"aria-hidden": "true"} «
    %li.active
      %a{href: "#"}
        1
        %span.sr-only (current)
    %li
      %a{href: "#"} 2
    %li
      %a{href: "#"} 3
    %li
      %a{href: "#"} 4
    %li
      %a{href: "#"} 5
    %li
      %a{"aria-label": "Next", href: "#"}
        %span{"aria-hidden": "true"} »
